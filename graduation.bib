pages = {276--286},
title = {{Supercritical Fluid Science and Technology}},
url = {http://pubs.acs.org/doi/abs/10.1021/bk-1989-0406.ch018{\%}5Cnhttp://scholar.google.com/scholar?hl=en{\&}btnG=Search{\&}q=intitle:Thermodynamic+Analysis+of+Corrosion+of+Iron+Alloys+in+Supercritical+Water{\#}0{\%}5Cnhttp://pubs.acs.org/doi/book/10.1021/bk-1989-0406},
