@article{Yang2014,
abstract = {Based on previous experiments and the volume of fluid (VOF) multiphase model, the growth characteristics of a single bubble have been numerically investigated in a rectangular pool (10??10??25 mm3) under microgravity. The transport of mass and energy during phase change was realized by source terms of the mass and energy equations through user-defined functions (UDF). Under microgravity, the results show that the temperature and the streamline field distribution around the bubble are significantly changed as compared to the ones of terrestrial conditions. The temperature profile at the two-phase interface is no longer a uniform distribution, and the Marangoni flows are more obvious at the two-phase interface. The effects of gravity on the detachment of the bubble are significant: the bubble does not immediately detach from the heating wall under microgravity conditions. The surface tension gradient caused by the Marangoni effect is more significant at lower microgravity. Bubble growth is more complex under microgravity conditions than normal gravity conditions, and it is related to the magnitude of the microgravity: the lower the microgravity, the higher the bubble growth rate. Furthermore, under microgravity, the bubble diameter changes differently, and the fluctuation amplitude of the heat transfer coefficient increases with increasing microgravity. ?? 2014 IAA. Published by Elsevier Ltd. All rights reserved.},
author = {Yang, Yan and Pan, Liang Ming and Xu, Jian Jun},
doi = {10.1016/j.actaastro.2014.04.005},
file = {:Users/zuozhongqi/Documents/Papers/Effects of microgravity on marangoni convection and growth characteristic of a single bubble/2014{\_}Effects of microgravity on marangoni convection and growth characteristic of a single bubble.pdf:pdf},
issn = {00945765},
journal = {Acta Astronautica},
keywords = {Bubble growth,Microgravity,VOF model},
number = {1},
pages = {129--139},
publisher = {Elsevier},
title = {{Effects of microgravity on marangoni convection and growth characteristic of a single bubble}},
url = {http://dx.doi.org/10.1016/j.actaastro.2014.04.005},
volume = {100},
year = {2014}
}
@phdthesis{2014a,
abstract = {液体推进剂贮箱作为航天器推进系统的重要组成部分,其主要功能是为航天器提供不夹气的推进剂。在微重力环境下,表面张力效应占主要地位,液体的性质与常重力下明显不同,这对贮箱内液体的传输和重定位提出了新的挑战。因此研究高效、简单的液体推进剂贮箱具有重要意义。对于航天器发动机点火造成的液体重定位问题,利用国家微重力实验室百米落塔设施对一种球型板式贮箱缩比模型进行了微重力实验,研究了液体在加速度方向沉底情况下的重定位过程。分别对正向、反向和侧向加速度下不同充液率时液体重定位过程进行了实验研究,得到了重定位过程中液体位置的变化,重定位完成后稳定的气液平衡界面以及一系列的重定位时间。我们发现液体重定位完成后,当充液率大于2{\%}时,液体在加速度方向明显沉降。因此在Bond数大于2.5情况下,应该在气口位置安装导流板将液体输送到液口,在蓄液叶片靠近支柱处打通孔,加大液体在不同叶片间的流通,从而减少重定位时间。对于胶囊型板式贮箱内液体传输问题,利用Flow3D软件对具有平行导流板贮箱内的毛细流动进行仿真分析。当充液率为15{\%}时,将落塔实验结果和仿真结果进行了对比,验证了整个仿真程序的可行性。通过对比不同时刻导流板上液体位置,研究不同充液率、不同导流板数量和不同间隙对导流板传输性能的影响。改变导流板结构,比较不同结构下前缘爬升速度和某个固定截面以下的液量,分析导流板结构参数对传输性能的影响,对导流板优化设计提出几点建议。},
author = {芮伟},
keywords = {Flow3D,板式贮箱,毛细流动,落塔实验,重定位},
publisher = {东北大学},
title = {板式贮箱内在轨液体传输和界面现象的数值模拟和实验研究},
year = {2014}
}
@article{Mei2002,
abstract = {The present work investigates two approaches for force evaluation in the lattice Boltzmann equation: the momentum-exchange method and the stress-integration method on the surface of a body. The boundary condition for the particle distribution functions on curved geometries is handled with second-order accuracy based on our recent works [Mei et al., J. Comput. Phys. 155, 307 (1999); ibid. 161, 680 (2000)]. The stress-integration method is computationally laborious for two-dimensional flows and in general difficult to implement for three-dimensional flows, while the momentum-exchange method is reliable, accurate, and easy to implement for both two-dimensional and three-dimensional flows. Several test cases are selected to evaluate the present methods, including: (i) two-dimensional pressure-driven channel flow; (ii) two-dimensional uniform flow past a column of cylinders; (iii) two-dimensional flow past a cylinder asymmetrically placed in a channel (with vortex shedding); (iv) three-dimensional pressure-driven flow in a circular pipe; and (v) three-dimensional flow past a sphere. The drag evaluated by using the momentum-exchange method agrees well with the exact or other published results.},
author = {Mei, Renwei and Yu, Dazhi and Shyy, Wei and Luo, Li Shi},
doi = {10.1103/PhysRevE.65.041203},
file = {:Users/zuozhongqi/Documents/Papers/Force evaluation in the lattice Boltzmann method involving curved geometry/2002{\_}Force evaluation in the lattice Boltzmann method involving curved geometry.pdf:pdf},
isbn = {1063-651X$\backslash$r1095-3787},
issn = {15393755},
journal = {Physical Review E - Statistical, Nonlinear, and Soft Matter Physics},
number = {4},
pages = {1--14},
pmid = {12005812},
title = {{Force evaluation in the lattice Boltzmann method involving curved geometry}},
volume = {65},
year = {2002}
}
