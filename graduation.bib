issn = {16134982},
journal = {Microfluidics and Nanofluidics},
keywords = {Bubble/droplet dynamics,Electrokinetics,Interfacial phenomena,Interfacial slip,Lattice Boltzmann method,Microfluidics,Multiphase flows,Numerical simulation},
number = {1},
