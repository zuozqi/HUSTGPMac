@article{Zhang2011,
abstract = {The lattice Boltzmann method (LBM) has experienced tremendous advances$\backslash$nand has been well accepted as a useful method to simulate various fluid$\backslash$nbehaviors. For computational microfluidics, LBM may present some$\backslash$nadvantages, including the physical representation of microscopic$\backslash$ninteractions, the uniform algorithm for multiphase flows, and the$\backslash$neasiness in dealing with complex boundary. In addition, LBM-like$\backslash$nalgorithms have been developed to solve microfluidics-related processes$\backslash$nand phenomena, such as heat transfer, electric/magnetic field, and$\backslash$ndiffusion. This article provides a practical overview of these LBM$\backslash$nmodels and implementation details for external force, initial condition,$\backslash$nand boundary condition. Moreover, recent LBM applications in various$\backslash$nmicrofluidic situations have been reviewed, including microscopic$\backslash$ngaseous flows, surface wettability and solid-liquid interfacial slip,$\backslash$nmultiphase flows in microchannels, electrokinetic flows, interface$\backslash$ndeformation in electric/magnetic field, flows through porous structures,$\backslash$nand biological microflows. These simulations show some examples of the$\backslash$ncapability and efficiency of LBM in computational microfluidics.},
author = {Zhang, Junfeng},
doi = {10.1007/s10404-010-0624-1},
file = {:Users/zuozhongqi/Documents/Papers/Lattice Boltzmann method for microfluidics Models and applications/2011{\_}Lattice Boltzmann method for microfluidics Models and applications.pdf:pdf},
