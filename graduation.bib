journal = {Journal of Computational Physics},
keywords = {Free surface flows,Level set,Surface tension,Volume-of-fluid},
number = {2},
pages = {506--523},
title = {{A numerical method for capillarity-dominant free surface flows}},
volume = {221},
year = {2007}
}
