author = {Martys, Nicos and Chen, Hudong},
doi = {10.1103/PhysRevE.53.743},
file = {:Users/zuozhongqi/Documents/Papers/Simulation of multicomponent fluids in complex three-dimensional geometries by the lattice Boltzmann method/1996{\_}Simulation of multicomponent fluids in complex three-dimensional geometries by the lattice Boltzmann method.pdf:pdf},
