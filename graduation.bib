@article{Saka2009,
abstract = {A review.  Supercrit. fluid science and technol. has been considered to be an environmentally benign treatment.  Therefore, its technol. was applied for a conversion of biomass to useful fuels and chems. so as to mitigate the environmental loading.  For example, supercrit. or subcrit. water treatment has demonstrated that lignocellulosics can be decompd. to be lignin-derived products for useful arom. chems. and carbohydrate-derived products for subsequent ethanol fermn. as well as acetic acid fermn. to produce bioethanol.  If this treatment is prolonged, lignocellulosics were found to be converted to org. acids which can be further converted to methane as one of the useful biofuels.  If alcs. such as methanol, ethanol etc. are used in their supercrit. condition for lignocellulosics, some other useful products were achieved and its liquefied products were found to have a potential as liq. biofuel, while methanol for oils and fats are used, biodiesel can be produced under non-catalytic conditions.  In addn., a potential of carboxylate ester and di-Me carbonate in supercrit. state for biodiesel prodn. has been also demonstrated.  Consequently, our research achievements in supercrit. fluid science and technol. for various biomass resources were introduced for clean and green chem. for a sustainable environment. [on SciFinder(R)]},
author = {Saka, Shiro},
journal = {Journal of the Japan Institute of Energy},
number = {5},
pages = {362--368},
title = {{Pioneering Research on Biofuels by Supercritical Fluid Science and Technology}},
volume = {88},
year = {2009}
}
