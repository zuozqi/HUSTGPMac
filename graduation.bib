@article{Panzarella2004,
abstract = {This work studies the pressure control of a large cryogen storage tank in microgravity using a series of parametric numerical simulations. The complete governing equations in the liquid region are coupled to a lumped thermodynamic treatment of the vapor region. It is shown that even in microgravity buoyancy effects rapidly bring the vapor region up to the tank wall. Long-term pressurization studies show that natural convection still leads to considerable thermal stratification in the liquid. A subcooled liquid jet is used to control the pressure rise. At its lowest speed, the jet cannot penetrate far enough into the liquid region to cool the vapor because of retarding buoyancy forces. As the jet speed is increased, it is shown to be quite effective at disrupting the thermal stratification and reducing the tank pressure in a reasonable amount of time. ?? 2004 Elsevier Ltd. All rights reserved.},
author = {Panzarella, Charles and Plachta, David and Kassemi, Mohammad},
doi = {10.1016/j.cryogenics.2004.03.009},
file = {:Users/zuozhongqi/Documents/Papers/Pressure control of large cryogenic tanks in microgravity/2004{\_}Pressure control of large cryogenic tanks in microgravity.pdf:pdf},
isbn = {0011-2275},
issn = {00112275},
journal = {Cryogenics},
keywords = {CFD,Space cryogenics (F),Two-phase flow (C),Vapor pressure (C),Zero boil off},
number = {6-8},
pages = {475--483},
title = {{Pressure control of large cryogenic tanks in microgravity}},
volume = {44},
year = {2004}
}
@article{Ho2008,
abstract = {This paper presents a study on fluid flow and heat transfer of liquid hydrogen in a zero boil-off cryogenic storage tank in a microgravity environment. The storage tank is equipped with an active cooling system consisting of a heat pipe and a pump-nozzle unit. The pump collects cryogen at its inlet and discharges it through its nozzle onto the evaporator section of the heat pipe in order to prevent the cryogen from boiling off due to the heat leaking through the tank wall from the surroundings. A three-dimensional (3-D) finite element model is employed in a set of numerical simulations to solve for velocity and temperature fields of liquid hydrogen in steady state. Complex structures of 3-D velocity and temperature distributions determined from the model are presented. Simulations with an axisymmetric model were also performed for comparison. Parametric study results from both models predict that as the speed of the cryogenic fluid discharged from the nozzle increases, the mean or bulk cryogenic fluid speed increases linearly and the maximum temperature within the cryogenic fluid decreases. ?? 2007 Elsevier Ltd. All rights reserved.},
author = {Ho, Son H. and Rahman, Muhammad M.},
doi = {10.1016/j.cryogenics.2007.09.005},
file = {:Users/zuozhongqi/Documents/Papers/Three-dimensional analysis for liquid hydrogen in a cryogenic storage tank with heat pipe-pump system/2008{\_}Three-dimensional analysis for liquid hydrogen in a cryogenic storage tank with heat pipe-pump system.pdf:pdf},
issn = {00112275},
journal = {Cryogenics},
keywords = {Cryostats (F),Forced flow (C),Heat pipe (F),Liquid hydrogen (B),Zero boil-off (C)},
number = {1-2},
pages = {31--41},
title = {{Three-dimensional analysis for liquid hydrogen in a cryogenic storage tank with heat pipe-pump system}},
volume = {48},
year = {2008}
}
