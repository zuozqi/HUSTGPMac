isbn = {0021-9991},
issn = {00219991},
journal = {Journal of Computational Physics},
keywords = {capillary waves,drops and bubbles,interfaces,liquid bridges,multiphase flow,non-homogeneous flows,numerical simulation,pendant drops,reconnection of interfaces,sedimenting arrays},
pages = {423--456},
title = {{Volume-of-Fluid Interface Tracking with Smoothed Surface Stress Methods for Three-Dimensional Flows}},
